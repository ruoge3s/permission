{
    "name": "ruoge3s/permission",
    "description": "用于hyperf的permission组件",
    "keywords": [
        "hyperf",
        "permission",
        "permissions",
        "roles",
        "acl",
        "rbac",
        "security"
    ],
    "license": "MIT",
    "authors": [],
    "require": {
        "php": "^7.2",
        "ext-swoole": ">=4.4",
        "hyperf/cache": "^2.0.0",
        "hyperf/command": "^2.0.0",
        "hyperf/config": "^2.0.0",
        "hyperf/database": "~2.0.0",
        "hyperf/db-connection": "^2.0",
        "hyperf/utils": "~2.0.0"
    },
    "require-dev": {
        "hyperf/testing": "~2.0.0"
    },
    "autoload": {
        "psr-4": {
            "Qingliu\\Permission\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {}
    },
    "extra": {
        "hyperf": {
            "config": "Qingliu\\Permission\\ConfigProvider"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
